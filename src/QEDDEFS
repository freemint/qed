ifeq ($(qed),000)
TARGET = ./qed_new.app
CPU = 000
endif

ifeq ($(qed),02060)
TARGET = ./qed_new.app
CPU = 020-60
endif

ifeq ($(qed),030)
TARGET = ./qed_new.app
CPU = 030
endif

ifeq ($(qed),040)
TARGET = ./qed_new.app
CPU = 040
endif

ifeq ($(qed),060)
TARGET = ./qed_new.app
CPU = 060
endif

ifeq ($(qed),col)
TARGET = ./qed_new.app
CPU = v4e
endif

qedtargets = 000 02060 030 040 060 col
